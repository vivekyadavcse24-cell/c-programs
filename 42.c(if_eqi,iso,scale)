#include<stdio.h>
int main()
{
  int a,b,c;
printf(" enter sides of triangle:");
scanf("%d\n%d\n%d",&a,&b,&c);
if(a==b==c)
  printf("it is equilateral triangle");
else if(a==b||b==c||a==c)
  printf("it is isoscales triangle");
else if(a!=b!=c)
     printf("it is scalen triangle");
  else
  printf("invalid input\a");
return 0;
}
